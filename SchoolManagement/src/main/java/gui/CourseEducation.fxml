<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CourseEducationController">
	<children>
		<TableView fx:id="table" layoutX="14.0" layoutY="40.0" prefHeight="360.0">
			<columns>
				<TableColumn fx:id="id" prefWidth="75.0" resizable="false" text="ID" />
				<TableColumn fx:id="name" prefWidth="100.0" resizable="false" text="Name" />
				<TableColumn fx:id="startDate" prefWidth="100.0" resizable="false" text="Start date" />
				<TableColumn fx:id="endDate" prefWidth="100.0" resizable="false" text="End date" />
			</columns>
		</TableView>
		<VBox layoutX="400.0" prefHeight="600.0" prefWidth="200.0">
         <children>
            <VBox alignment="CENTER" prefHeight="600.0" prefWidth="300.0" spacing="10.0">
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
               <children>
                  <Button mnemonicParsing="false" onAction="#updateTableView" stylesheets="@application.css" text="Update Table" />
               </children>
            </VBox>
			</children>
		</VBox>
	</children>
</AnchorPane>
