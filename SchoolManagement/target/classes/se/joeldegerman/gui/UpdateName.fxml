<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="se.joeldegerman.gui.UpdateNameController">
   <children>
      <HBox alignment="CENTER" prefHeight="121.0" prefWidth="281.0" spacing="10.0">
         <children>
            <VBox alignment="CENTER">
               <children>
                  <Label text="Name" />
                  <TextField fx:id="nameTextField" />
               </children>
            </VBox>
            <Button fx:id="updateNameBtn" mnemonicParsing="false" onAction="#updateName" stylesheets="@application.css" text="Update" translateY="10.0" />
         </children>
      </HBox>
   </children>
</AnchorPane>
